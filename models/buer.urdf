      <mass value="0.8"/>
      <inertia ixx="0.04" ixy="0" ixz="0" iyy="0.04" iyz="0" izz="0.005"/>
    </inertial>
  </link>
  <joint name="leg1_calf_joint" type="revolute">
    <parent link="leg1_lap"/>
    <child link="leg1_calf"/>
    <origin xyz="0 0 1" rpy="0 0 1.5708"/> <!-- 90度=1.5708弧度 -->
    <axis xyz="1 0 0"/>
    <limit effort="100.0" velocity="10.0" lower="-1.57" upper="1.57" />
  </joint>

