<?xml version="1.0"?>
<robot name="buer">

  <!-- Head (Base link) -->
  <link name="head">
    <visual>
      <geometry>
        <mesh filename="obj/head.obj"/>
      </geometry>
      <!-- 材质可省略或补全 -->
    </visual>
    <collision>
      <geometry>
        <cylinder radius="1.0" length="1.0"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="8.0"/>
      <inertia ixx="1.5" ixy="0" ixz="0" iyy="1.5" iyz="0" izz="2.0"/>
    </inertial>
    <!-- IMU -->
    <sensor type="imu" name="imu">
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </sensor>
  </link>

  <!-- Leg 1 -->
  <link name="leg1_lap">
    <visual>
      <geometry>
        <mesh filename="obj/lap.obj"/>
      </geometry>
    </visual>
    <collision>
      <geometry>
        <mesh filename="obj/lap.obj"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0.5" rpy="0 0 0"/>
      <mass value="1.2"/>
      <inertia ixx="0.08" ixy="0" ixz="0" iyy="0.08" iyz="0" izz="0.01"/>
    </inertial>
  </link>
  <joint name="leg1_lap_joint" type="revolute">
    <parent link="head"/>
    <child link="leg1_lap"/>
    <origin xyz="0 0.5505528 0" rpy="-1.57 0 0"/>
    <axis xyz="1 0 0"/>
    <limit effort="100.0" velocity="10.0" lower="-0.5" upper="0.5"/>
  </joint>

  <link name="leg1_calf">
    <visual>
